{
  "extends": ["prettier", "tslint:latest", "tslint-react"],
  "rules": {
    "quotemark": [true, "single"],
    "no-submodule-imports": false,
    "semicolon": false,
    "no-console": false,
    "trailing-comma": false,
    "arrow-parens": false,
    "jsx-no-multiline-js": false,
    "jsx-no-lambda": false
  }
}
